<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="LCINSUREDSPECDAO">
	<select id="getLcinsuredByContNo" parameterClass="java.lang.String"
		resultClass="com.sinosoft.surrender.db.model.LcInsured">

		select trim(CONTNO) CONTNO, trim(INSUREDNO) INSUREDNO, trim(GRPCONTNO) GRPCONTNO, trim(PRTNO)
      PRTNO, trim(APPNTNO) APPNTNO, trim(MANAGECOM) MANAGECOM, trim(EXECUTECOM) EXECUTECOM,
      trim(FAMILYID) FAMILYID, trim(RELATIONTOMAININSURED) RELATIONTOMAININSURED,
      trim(RELATIONTOAPPNT) RELATIONTOAPPNT, trim(ADDRESSNO) ADDRESSNO, SEQUENCENO, NAME, trim(SEX)
      SEX, BIRTHDAY, trim(IDTYPE) IDTYPE, trim(IDNO) IDNO, trim(NATIVEPLACE) NATIVEPLACE,
      trim(NATIONALITY) NATIONALITY, RGTADDRESS, trim(MARRIAGE) MARRIAGE, MARRIAGEDATE, trim(HEALTH)
      HEALTH, STATURE, AVOIRDUPOIS, trim(DEGREE) DEGREE, trim(CREDITGRADE) CREDITGRADE,
      trim(BANKCODE) BANKCODE, BANKACCNO, ACCNAME, JOINCOMPANYDATE, STARTWORKDATE, POSITION, SALARY,
      trim(OCCUPATIONTYPE) OCCUPATIONTYPE, trim(OCCUPATIONCODE) OCCUPATIONCODE, WORKTYPE,
      PLURALITYTYPE, trim(SMOKEFLAG) SMOKEFLAG, trim(CONTPLANCODE) CONTPLANCODE, OPERATOR,
      trim(INSUREDSTAT) INSUREDSTAT, MAKEDATE, trim(MAKETIME) MAKETIME, MODIFYDATE, trim(MODIFYTIME)
      MODIFYTIME, trim(UWFLAG) UWFLAG, UWCODE, UWDATE, trim(UWTIME) UWTIME, BMI, INSUREDPEOPLES,
      CONTPLANCOUNT, DISKIMPORTNO, IVALIDATE, REMARK, trim(IMPARTFLAG) IMPARTFLAG,
      trim(MAININSUREDNO) MAININSUREDNO, RESIDENTTYPE, ESTSALARY, trim(REVENUETYPE) REVENUETYPE 
      from lcinsured a where a.contno= rpad(#contno#,20)

	</select>


	<select id="getLcinsuredInfos" parameterClass="java.util.Map"
		resultClass="com.sinosoft.surrender.db.model.LcInsured">

		select trim(CONTNO) CONTNO, trim(INSUREDNO) INSUREDNO, trim(GRPCONTNO) GRPCONTNO, trim(PRTNO)
      PRTNO, trim(APPNTNO) APPNTNO, trim(MANAGECOM) MANAGECOM, trim(EXECUTECOM) EXECUTECOM,
      trim(FAMILYID) FAMILYID, trim(RELATIONTOMAININSURED) RELATIONTOMAININSURED,
      trim(RELATIONTOAPPNT) RELATIONTOAPPNT, trim(ADDRESSNO) ADDRESSNO, SEQUENCENO, NAME, trim(SEX)
      SEX, BIRTHDAY, trim(IDTYPE) IDTYPE, trim(IDNO) IDNO, trim(NATIVEPLACE) NATIVEPLACE,
      trim(NATIONALITY) NATIONALITY, RGTADDRESS, trim(MARRIAGE) MARRIAGE, MARRIAGEDATE, trim(HEALTH)
      HEALTH, STATURE, AVOIRDUPOIS, trim(DEGREE) DEGREE, trim(CREDITGRADE) CREDITGRADE,
      trim(BANKCODE) BANKCODE, BANKACCNO, ACCNAME, JOINCOMPANYDATE, STARTWORKDATE, POSITION, SALARY,
      trim(OCCUPATIONTYPE) OCCUPATIONTYPE, trim(OCCUPATIONCODE) OCCUPATIONCODE, WORKTYPE,
      PLURALITYTYPE, trim(SMOKEFLAG) SMOKEFLAG, trim(CONTPLANCODE) CONTPLANCODE, OPERATOR,
      trim(INSUREDSTAT) INSUREDSTAT, MAKEDATE, trim(MAKETIME) MAKETIME, MODIFYDATE, trim(MODIFYTIME)
      MODIFYTIME, trim(UWFLAG) UWFLAG, UWCODE, UWDATE, trim(UWTIME) UWTIME, BMI, INSUREDPEOPLES,
      CONTPLANCOUNT, DISKIMPORTNO, IVALIDATE, REMARK, trim(IMPARTFLAG) IMPARTFLAG,
      trim(MAININSUREDNO) MAININSUREDNO, RESIDENTTYPE, ESTSALARY, trim(REVENUETYPE) REVENUETYPE 
      from lcinsured a where a.contno= rpad(#contno#,20) and a.insuredno = rpad(#insuredno#,24)

	</select>


</sqlMap>